{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": ["arn:aws:s3:::foamdino-test"]
    },
    {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": ["arn:aws:s3:::foamdino-test/*"]
    },
    {
      "Effect": "Allow",
      "Action": [
        "cloudwatch:putMetricData"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action" :[
        "sns:Publish"
      ],
      "Resource": "*"
    }
  ]
}
